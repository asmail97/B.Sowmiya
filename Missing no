public class Missing {
    public static void main(String[] args)
    {
        int arr[] = {1,2,3,5,6};
        int n = arr.length + 1;
        int total = n * (n + 1) / 2;
        int arrsum = 0;
        
        for(int num : arr)
        {
            arrsum = arrsum + num;
        }
          int miss = total - arrsum; 
           System.out.println(miss + ":Missing no");  
    }
}
