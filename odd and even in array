public class Oddarray {
    public static void main(String[] args)
    {
        //print odd and even element in array
        int arr[] = {1,2,3,4,5};
        for(int i = 0; i < arr.length; i++)
        {
            if(arr[i] % 2 == 0)
            {
                System.out.println("Even");
            }
            else
            {
               System.out.println("Odd");
            }
        }
    }
}
