public class printeven {
    public static void main(String[] args)
    {
        //print all even no upto 100
       for(int i = 0; i <= 100; i++)
       {
        if(i % 2 == 0)
        {
            System.out.print(i);
        }
        else
        {
            System.out.print("");
        }
       }
    }
}
