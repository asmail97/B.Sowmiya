import java.util.Scanner;
public class lcm {
    static int gcd(int a, int b)
{
  while (b != 0)
  {
    int temp = b;
    b = a % b;
    a = temp;
  }
  return a;
}
    public static int lcm(int a, int b)
    {
    return(a * b) / gcd(a,b);
    }
    public static void main(String[] args)
    {
    Scanner sc = new Scanner(System.in);
    
    System.out.print("");
    int a = sc.nextInt();
    System.out.print("");
    int b = sc.nextInt();
    int result = lcm(a, b);
    System.out.print("LCM = " + result);
}
}


    
       
