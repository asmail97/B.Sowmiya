public class grade {
    public static void main(String[] args)
    {
        int a = 99;
        if(a >= 90)
        {
            System.out.print("Grade A");
        }
        else if(a >= 80)
        {
            System.out.print("Grade B");
        }
        else
        {
            System.out.print("Grade C");
        }
    }
}
