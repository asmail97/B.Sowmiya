public class avgarray {
    public static void main(String[] args)
    {
        //average
        int arr[] = {1,2,3,4,4};
        int add = 0;
        for(int i = 0; i < arr.length; i++)
        {
           add += arr[i];
        }
         double ave = (double)add / arr.length;
          System.out.print(ave);
        
    }
}
